{
    "name": "litecms/portfolio",
    "description": "Portfolio package for litecms.",
    "keywords": ["lavalite", "Portfolio"],
    "license": "MIT",
    "type": "lavalite-package",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Litecms\\Portfolio\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Litecms\\Portfolio\\Providers\\PortfolioServiceProvider"
            ],
            "aliases": {
                "Portfolio"    : "Litecms\\Portfolio\\Facades\\Portfolio"
            }
        }
    },
    "minimum-stability": "dev"
}
